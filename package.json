    "dotenv": "^16.5.0",
    "image-resize-compress": "^2.1.1",
    "js-cookie": "^3.0.5",
    "prettier": "^3.2.4",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primereact": "^10.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.20.1",
    "typescript": "^5.5.3",
    "uuid": "^9.0.1",
    "watermarkjs": "^2.1.1"
  },
  "devDependencies": {
    "@be-tech/watermarkjs": "^1.0.2-beta.0",
